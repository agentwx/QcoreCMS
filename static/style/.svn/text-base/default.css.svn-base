.ui-menu {
  height: 35px;
  line-height: 35px;
  font-size: 15px;
  border-radius: 6px;
  margin: 0;
  padding: 0 6px;
  border: 1px solid #acacac;
  box-shadow: 0 0 4px #999;
  background: #e6e6e6;
  background: -webkit-linear-gradient(#efefef, #fefefe 1px, #e6e6e6);
  background: -moz-linear-gradient(#efefef, #fefefe 1px, #e6e6e6);
  background: -ms-linear-gradient(#efefef, #fefefe 1px, #e6e6e6);
  background: -o-linear-gradient(#efefef, #fefefe 1px, #e6e6e6);
  background: linear-gradient(#efefef, #fefefe 1px, #e6e6e6);
  -pie-background: linear-gradient(#efefef, #fefefe 1px, #e6e6e6);
  behavior: url("PIE.htc");
}
.ui-menu li {
  list-style: none;
  margin: 0;
  padding: 0;
  float: left;
}
.ui-menu li a {
  display: inline-block;
  zoom: 1;
  *display: inline;
  overflow: visible !ie;
  text-decoration: none;
  color: #666;
  padding: 0 8px 0 10px;
  height: 35px;
  border-right: 1px solid #e6e6e6;
  background: #fefefe;
  background: -webkit-linear-gradient(#fefefe, #e6e6e6);
  background: -moz-linear-gradient(#fefefe, #e6e6e6);
  background: -ms-linear-gradient(#fefefe, #e6e6e6);
  background: -o-linear-gradient(#fefefe, #e6e6e6);
  background: linear-gradient(#fefefe, #e6e6e6);
  -pie-background: linear-gradient(#fefefe, #e6e6e6);
  behavior: url("PIE.htc");
}
.ui-menu li a:hover {
  color: #fd971f;
}
.ui-menu li a.select {
  background: #e6e6e6;
  background: -webkit-linear-gradient(#e6e6e6, #e9e9e9);
  background: -moz-linear-gradient(#e6e6e6, #e9e9e9);
  background: -ms-linear-gradient(#e6e6e6, #e9e9e9);
  background: -o-linear-gradient(#e6e6e6, #e9e9e9);
  background: linear-gradient(#e6e6e6, #e9e9e9);
  -pie-background: linear-gradient(#e6e6e6, #e9e9e9);
  behavior: url("PIE.htc");
}
.ui-menu li a.select:hover {
  color: #666;
}
.ui-menu-pills {
  height: 35px;
  line-height: 35px;
  font-size: 15px;
  margin: 0;
  padding: 0 6px;
}
.ui-menu-pills li {
  list-style: none;
  margin: 0;
  padding: 0 1px;
  float: left;
  font-weight: bold;
}
.ui-menu-pills li a {
  display: inline-block;
  zoom: 1;
  *display: inline;
  overflow: visible !ie;
  text-decoration: none;
  color: #666;
  padding: 0 14px;
  height: 35px;
  background: none;
  border-radius: 6px;
  behavior: url("PIE.htc");
}
.ui-menu-pills li a:hover {
  color: #fd971f;
  background: #eeeeee;
}
.ui-menu-pills li a.select,
.ui-menu-pills li a.select:hover {
  background: #fd971f;
  color: #fff;
  border-radius: 6px;
  behavior: url("PIE.htc");
}
.ui-menu-tabs {
  height: 35px;
  line-height: 35px;
  font-size: 15px;
  margin: 0;
  padding: 0 6px;
  border-bottom: 1px solid #999;
}
.ui-menu-tabs li {
  list-style: none;
  margin: 0;
  margin-bottom: -1px;
  padding: 0 1px;
  float: left;

.ui-table {
  border: 1px solid #9e9e9e;
  border-radius: 6px;
  background: #efefef;
  behavior: url("PIE.htc");
}
.ui-table table {
  width: 100%;
}
.ui-table th {
  height: 40px;
  line-height: 40px;
  text-align: left;
  padding-left: 6px;
}
.ui-table td {
  padding: 6px;
}
body {
  background: url(img/bg.jpg);
  font: 16px/1.62 '微软雅黑', 'Hiragino Sans GB', 'Helvetica Neue', Helvetica, Arial, sans-serif;
}
#main {
  position: relative;
  overflow: hidden;
}
#header {
  height: 50px;
  border-top: 1px solid #777;
  border-bottom: 1px solid #000;
  box-shadow: 0 1px 3px #444;
  z-index: 3;
  position: relative;
  overflow: hidden;
  background: #505153;
  background: -webkit-linear-gradient(#505153, #363a3d);
  background: -moz-linear-gradient(#505153, #363a3d);
  background: -ms-linear-gradient(#505153, #363a3d);
  background: -o-linear-gradient(#505153, #363a3d);
  background: linear-gradient(#505153, #363a3d);
  -pie-background: linear-gradient(#505153, #363a3d);
  behavior: url("PIE.htc");
}
#header h1 {
  position: absolute;
  color: #efefef;
  bottom: 0px;
  left: 20px;
  margin: 0;
  padding: 0;
  font-size: 30px;
}
#header ul {
  position: absolute;
  bottom: 0px;
  right: 8px;
}
#header ul li {
  display: block;
  float: left;
}
#header ul li a {
  color: #fff;
  float: left;
  display: block;
  margin: 0 5px;
  text-decoration: none;
  color: #999;
}
#header ul li a.btn-warning {
  color: #fff;
}
#menu {
  background: url(img/bg_tile.jpg);
  float: left;
  width: 200px;
  z-index: 1;
  position: relative;
  overflow: hidden;
}
#menu span {
  position: absolute;
  bottom: 0px;
  right: 8px;
  color: #999;
  cursor: pointer;
  font-size: 30px;
}
#menu .title {
  border-bottom: 1px solid #666d76;
  filter: progid:dximagetransform.microsoft.gradient(enabled='true', startColorstr='#4C000000', endColorstr='#4C000000');
  background: rgba(0, 0, 0, 0.3);
  position: relative;
  height: 35px;
}
#menu .title h3 {
  display: block;
  color: #fff;
  font-weight: bold;
  font-size: 16px;
  border-bottom: 1px solid #3e424b;
  margin: 0;
  height: 35px;
  line-height: 35px;
  padding: 0 6px;
  cursor: pointer;
}
#menu dl {
  margin: 0 5px;
  margin-bottom: 10px;
  position: relative;
  *border-top: 1px solid #52555c;
}
#menu dl a {
  display: block;
  width: 100%;
  color: #efefef;
  font-size: 13px;
  padding: 4px;
  border-bottom: 1px solid #666d76;
  text-decoration: none;
}
#menu dl a:hover,
#menu dl a.select {
  color: #fd971f;
}
#menu dt {
  position: relative;
  margin: 10px 8px 0 8px;
  color: #e0e0e0;
  font-size: 14px;
  background: url(img/bg_tile.jpg);
  padding: 0 6px;
  height: 25px;
  line-height: 25px;
  font-weight: bold;
  border-radius: 6px 6px 0 0;
  border: 1px solid #333;
  border-bottom: 1px solid #666d76;
  box-shadow: 0 -1px 2px #666666;
  cursor: pointer;
  behavior: url("PIE.htc");
}
#menu dt span {
  bottom: 3px;
}
#menu dd {
  margin: 0 14px;
}
#wrap {
  position: absolute;
  z-index: 2;
  box-shadow: -6px 0 6px #444444;
  overflow: hidden;
  background: url(img/bg.jpg);
  behavior: url("PIE.htc");
}
#wrap .loading {
  position: absolute;
  z-index: 3;
  width: 300px;
  height: 80px;
  line-height: 80px;
  box-shadow: 0 2px 6px #444;
  top: 0;
  background: url(img/bg.jpg);
  border-radius: 0 0 6px 6px;
  padding: 0;
  text-align: center;
  display: none;
  behavior: url("PIE.htc");
}
.wrap {
  padding: 20px;
}
